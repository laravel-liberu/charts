    "keywords": [
        "laravel-liberu",
        "charts",
        "laravel",
        "chartjs"
    ],
    "homepage": "https://github.com/laravel-liberu/charts",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Adrian Ocneanu",
        "email": "aocneanu@gmail.com",
        "homepage": "https://laravel-liberu.com",
        "role": "Developer"
    }],
    "require": {
        "laravel/framework": "^7.0|^8.0|^9.0|^10.0",
        "laravel-liberu/helpers": "^2.0",
        "php": ">=7.4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelLiberu\\Charts\\": "src/"
        }
    },
    "extra": {
